{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=App_Data/squirrel.db"
  },
  "Database": {
    "Provider": "SQLite",
    "AutoMigrate": true,
    "SeedData": true
  },
  "Caching": {
    "Provider": "Memory"
  }
}
