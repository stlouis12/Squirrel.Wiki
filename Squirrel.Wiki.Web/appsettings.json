{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=App_Data/squirrel.db"
  },
  "Database": {
    "Provider": "SQLite",
    "AutoMigrate": true,
    "SeedData": true
  },
  "Caching": {
    "Provider": "Memory"
  }
}
